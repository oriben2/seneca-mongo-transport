  "author": "Ori Ben Aharon (http://github.com/oriben2)",
  "contributors": [
    "Ori Ben Aharon (http://github.com/oriben2)"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "4.14.1",
    "mongomq": "^0.3.7",
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "chai": "^3.5.0",
    "seneca": "plugin",
    "seneca-transport-test": "~0.1.3"
  },
  "files": [
    "README.md",
    "LICENSE",
    "lib"
  ],
  "pre-commit": [
